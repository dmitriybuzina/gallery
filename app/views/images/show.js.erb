$('#image-content').html("<%= j render 'image_modal', image: @image %>");
$('#image-modal').modal('show');

history.replaceState({}, "<%= @image.name %>", "<%= request.original_url %>");

$('#image-modal').on("hidden.bs.modal", function () {
    history.back(-1)
});

